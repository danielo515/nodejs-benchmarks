{
    "env": {
        "es6": true,
        "commonjs": true,
        "node": true
    },
    "ecmaFeatures": {
        "modules": true,
        "experimentalObjectRestSpread": false
    },
    "rules": {
        "no-unused-vars": "error",
        "strict": "error",
        "no-undef": "error",
        "semi": 2,
        "indent": [
            "error",
            4
        ]
    }
}